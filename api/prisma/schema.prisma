
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model autenticacao {
  idautenticacao Int                  @id @default(autoincrement())
  email          String?              @db.VarChar(100)
  senha          String?              @db.VarChar(80)
  googleId       String?              @db.Text()
}